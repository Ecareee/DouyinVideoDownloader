{
  "name": "douyin-video-downloader",
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently -n api,worker -c blue,green \"pnpm --filter @pkg/api dev\" \"pnpm --filter @pkg/worker dev\"",
    "build": "pnpm --filter web-ui build",
    "start": "NODE_ENV=production concurrently -n api,worker \"pnpm --filter @pkg/api dev\" \"pnpm --filter @pkg/worker dev\"",
    "dev:api": "pnpm --filter @pkg/api dev",
    "dev:worker": "pnpm --filter @pkg/worker dev",
    "dev:web": "pnpm --filter @app/web-ui dev",
    "dev:desktop": "pnpm --filter @app/desktop dev",
    "db:generate": "pnpm --filter @pkg/api db:generate",
    "db:push": "pnpm --filter @pkg/api db:push",
    "db:migrate": "pnpm --filter @pkg/api db:migrate"
  },
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}