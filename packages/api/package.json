{
  "name": "@pkg/api",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e ../../.env -- node --import tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "db:generate": "dotenv -e ../../.env -- prisma generate",
    "db:push": "dotenv -e ../../.env -- prisma db push",
    "db:migrate": "dotenv -e ../../.env -- prisma migrate dev"
  },
  "dependencies": {
    "@pkg/shared": "workspace:*",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bullmq": "^5.66.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "ws": "^8.18.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv-cli": "^11.0.0",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  }
}